#!/bin/bash

# Load xrandr customisation
$HOME/.screenlayout/default.sh

# Add any programs that should start before BSPWM here
$HOME/.fehbg
mpd &
numlockx &
dropbox &
discord &

# Enable popups and start notifications for mpd
$HOME/.scripts/popups
mpd-notification -m ${HOME}/Music &

# Show polybar
$HOME/.scripts/startpoly
